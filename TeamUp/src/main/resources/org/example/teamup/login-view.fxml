<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.teamup.ApplicationController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label fx:id="welcomeText"/>
  <Text fx:id="textoEmail" text="Ingrese su correo" />
  <TextField fx:id="email"/>
  <Text fx:id="textoContraseña" text="Ingrese su contraseña"/>
  <TextField fx:id="password" />
  <Text fx:id= "enlace_register" text="Si no te has registrado clica aquí" onMouseClicked="cambiarRegister"/>
  <Button text="Login" onAction="#onHelloButtonClick"/>
</VBox>
